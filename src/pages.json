{
	// 组件自动导入
	  "easycom": {
	    "autoscan": true,
	    "custom": {
	      // uni-ui 规则如下配置  
	      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue" 
	    }
	  },
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "某某省第一人民医院",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/register/register",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/video/video",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F7F7F7",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"backgroundColor": "#ffffff",
		"boderStyle": "black",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/tabbar/index.png",
				"selectedIconPath": "static/tabbar/indexselected.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/register/register",
				"iconPath": "static/tabbar/register.png",
				"selectedIconPath": "static/tabbar/registerselected.png",
				"text": "挂号"
			},
			{
				"pagePath": "pages/video/video",
				"iconPath": "static/tabbar/video.png",
				"selectedIconPath": "static/tabbar/videoselected.png",
				"text": "视频"
			},
			{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/tabbar/mine1.png",
				"selectedIconPath": "static/tabbar/mine1selected.png",
				"text": "我的"
			}
		]
	}
}